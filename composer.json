{
  "name": "tarundubey/laravel-prometheus-client",
  "description": "A prometheus exporter for Laravel",
  "license": "proprietary",
  "authors": [
    {
      "name": "Tarun Dubey",
      "email": "tarunkumardubey6793@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/support": "^5.3",
    "illuminate/routing": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "tarundubey\\LaravelPrometheusExporter\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "tarundubey\\LaravelPrometheusExporter\\PrometheusServiceProvider"
      ],
      "aliases": {
        "Prometheus": "tarundubey\\LaravelPrometheusExporter\\PrometheusFacade"
      }
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "mockery/mockery": "^0.9.5"
  }
}
